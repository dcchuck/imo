<!DOCTYPE HTML>
<html>
  <head>
    <title>IMO</title>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>

    <!-- Header -->
      <header id="header">
      </header>

    <!-- Nav -->
      <nav id="nav">
        <ul class="links">
        </ul>
      </nav>

    <!-- Main -->
      <section id="main" class="wrapper">
        <div class="container">

          <%= yield %>

        </div>
      </section>

    <!-- Footer -->
      <footer id="footer">
        <div class="inner">
          <ul class="copyright">
          </ul>
        </div>
      </footer>
    </body>
</html>
